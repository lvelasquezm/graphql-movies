{
  "name": "graphql-movies",
  "version": "1.0.0",
  "description": "A GraphQL services/API to fetch movies information",
  "main": "index.js",
  "author": "lvelasquezm",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphql-yoga": "1.14.10",
    "jsonwebtoken": "^8.5.1",
    "prisma-binding": "2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "env-cmd": "^9.0.3",
    "nodemon": "^1.19.1"
  },
  "scripts": {
    "start:local": "env-cmd -f ./config/local.env nodemon index.js --ext js,graphql --exec babel-node",
    "get-schema": "prisma generate",
    "deploy": "prisma deploy -e config/prod.env",
    "deploy:local": "prisma deploy -e config/local.env"
  }
}
